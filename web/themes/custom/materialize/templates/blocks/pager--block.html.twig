<div class="row">
  <div class="col s12">
    {% for obj in data %}
    <a class="btn-large blue darken-4 white-text pager-block-item {{ obj.class }}" href="{{ obj.href|raw }}">
      <span class="pager-prefix">&laquo; </span>{{ obj.label }}<span class="pager-suffix"> &raquo;</span>
    </a>
    {% endfor %}
  </div>
</div>
